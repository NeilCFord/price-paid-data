#!/usr/bin/env gnuplot

set terminal png font "helvetica,14" size 1600,1200 transparent truecolor
set output "/dev/stdout"
set key off
set view map

set xlabel "Month"
set xdata time
set timefmt "%Y-%md"
set format x "%Y-%m"

set ylabel "Price (Â£10k)"

set palette defined (\
0 '#fff7ec',\
1 '#fee8c8',\
2 '#fdd49e',\
3 '#fdbb84',\
4 '#fc8d59',\
5 '#ef6548',\
6 '#d7301f',\
7 '#b30000',\
8 '#7f0000')

plot '/dev/stdin' using 1:2:3 with image
